{
  "name": "@klownar/avito-monitor-v2",
  "version": "1.0.0",
  "author": "klonwar",
  "repository": "https://github.com/klonwar/avito-monitor-v2",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach --all --topological run build",
    "lint": "yarn workspaces foreach --all run lint",
    "postinstall": "husky || true",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/types": "19.8.1",
    "@eslint/js": "^9.32.0",
    "@innocells/branch-naming-check": "1.0.0",
    "@types/node": "^24.2.0",
    "commitlint-plugin-regex-match": "^1.2.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "16.1.4",
    "pinst": "^3.0.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "packageManager": "yarn@4.9.2",
  "engines": {
    "node": ">=22.0.0",
    "npm": "please-use-yarn",
    "yarn": ">=4.0.0"
  }
}
